{
	"name": "svelte-portfolio",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host 0.0.0.0 --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --check . && eslint .",
		"format": "prettier --plugin=prettier-plugin-svelte --plugin=prettier-plugin-tailwindcss --plugin=prettier-plugin-organize-imports . --write .",
		"prepare": "husky"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^3.1.7",
		"@skeletonlabs/skeleton-svelte": "^1.3.1",
		"@sveltejs/adapter-auto": "^6.0.2",
		"@sveltejs/adapter-vercel": "^5.8.2",
		"@sveltejs/kit": "^2.27.3",
		"@sveltejs/vite-plugin-svelte": "^6.1.1",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.32.4",
		"husky": "^9.1.7",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^4.2.0",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.0",
		"svelte-check": "^4.3.1",
		"tailwindcss": "^4.1.11",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vercel": "^44.7.3",
		"vite": "^7.1.1",
		"vitest": "^3.0.2"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.5.3",
		"@tailwindcss/vite": "^4.1.11",
		"@vercel/postgres": "^0.10.0"
	},
	"engines": {
		"node": "22.x"
	}
}